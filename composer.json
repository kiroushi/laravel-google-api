{
    "name": "kiroushi/laravel-google-api",
    "description": "Laravel Wrapper for PHP Google Api Client",
    "homepage": "https://github.com/kiroushi/laravel-google-api",
    "keywords": ["laravel", "google"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier PÃ©rez"
        }
    ],
    "require": {
        "php" : "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Kiroushi\\LaravelGoogleApi\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Kiroushi\\LaravelGoogleApi\\GoogleApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
